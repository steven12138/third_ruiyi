{
    "name": "third_ruiyi_electron",
    "version": "1.0.10",
    "description": "第三方睿易云客户端 powered by steven",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "build": "electron-builder"
    },
    "repository": "https://github.com/electron/electron-quick-start",
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "author": "GitHub",
    "license": "CC0-1.0",
    "devDependencies": {
        "electron": "^11.0.4"
    },
    "build": {
        "electronDownload": {
            "mirror": "https://npm.taobao.org/mirrors/electron/"
        },
        "productName": "睿易云steven",
        "appId": "xyz.steven12138.third_ruiyi",
        "win": {
            "icon": "./icon.ico",
            "target": [
                "nsis"
            ]
        },
        "nsis": {
            "installerIcon": "icon.ico",
            "uninstallerIcon": "icon.ico",
            "uninstallDisplayName": "睿易云steven 卸载",
            "oneClick": false,
            "allowToChangeInstallationDirectory": true,
            "allowElevation": true,
            "createDesktopShortcut": true
        }
    }
}